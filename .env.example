# DSpace API Configuration
# Copy this file to .env and fill in your values
# NEVER commit .env files with real credentials to version control

# DSpace REST API Base URL
VITE_API_URL=http://localhost:8080/server/api

# Auto-login (for development only - NEVER use in production)
# Set VITE_AUTO_LOGIN=true to enable auto-login during development
VITE_AUTO_LOGIN=false
VITE_DSPACE_USER=
VITE_DSPACE_PASSWORD=

# Application Settings
VITE_APP_TITLE=المستودع الرقمي - مكتبة الملك فهد الوطنية
VITE_APP_TITLE_EN=Digital Repository - King Fahd National Library

# Default Language (ar or en)
VITE_DEFAULT_LOCALE=ar

# Enable/Disable features
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false
